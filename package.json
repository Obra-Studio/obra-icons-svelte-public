{
	"name": "obra-icons-svelte",
	"version": "1.2.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package --input src/package && publint",
		"prepublishOnly": "pnpm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate:icons": "vite-node --config 'scripts/vite-node.config.ts' scripts/icons.ts",
		"generate:keywords": "vite-node --config 'scripts/vite-node.config.ts' scripts/keywords.ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.55.0 || ^4.0.0"
	},
	"devDependencies": {
		"@orama/orama": "=1.2.8",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.22.4",
		"@sveltejs/package": "^2.0.0",
		"@types/node": "^20.7.0",
		"@types/prettier": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"desm": "^1.3.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"just-split": "^3.2.0",
		"ofetch": "^1.3.3",
		"openai": "^4.10.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"publint": "^0.1.9",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-copy": "^1.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-node": "^0.34.5"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
